{
  "name": "grid-react-app",
  "version": "0.2.0",
  "private": true,
  "homepage": "./",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "engineStrict": true,
  "scripts": {
    "bootstrap": "mkdirp src/settings && ncp default.settings.json src/settings/index.json",
    "start": "webpack serve --mode development",
    "schema": "mkdirp build && npx ts-schema src/schema.ts build && yarn analytics-schema",
    "analytics-schema": "npx tsc src/analytics-schema.ts --moduleResolution node --skipLibCheck --outFile /dev/stdout -m amd | node -e 'a={};eval(\"define=(_,d,c)=>{c(a,a,...d.slice(2).map(require));console.log(JSON.stringify(a.default))};\"+require(\"fs\").readFileSync(\"/dev/stdin\",\"utf8\"))' > build/analytics-schema.json",
    "prodbuild": "cross-env GENERATE_SOURCEMAP=false webpack --mode production && ncp package.json build/",
    "build": "rimraf build && yarn prodbuild && yarn schema && phy app build",
    "pub": "phy app publish",
    "settings": "phy app settings",
    "upload-description": "phy app upload-description",
    "connect": "phy dev ws"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@ombori/grid-reports": "^3.37.4",
    "@phygrid/hub-client": "^4.2.400",
    "assert": "^2.1.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.27.0",
    "@emotion/babel-preset-css-prop": "^11.12.0",
    "@phygrid/cli": "^4.2.400",
    "@phygrid/ts-schema": "^4.2.400",
    "@svgr/webpack": "^8.1.0",
    "babel-loader": "^10.0.0",
    "babel-preset-react-app": "^10.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "^16.4.7",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "https-browserify": "^1.0.0",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mkdirp": "^3.0.1",
    "ncp": "^2.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "react-app-rewired": "^2.2.1",
    "react-dev-utils": "^12.0.1",
    "react-is": "^19.1.0",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "url": "^0.11.4",
    "url-loader": "^4.1.1",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
