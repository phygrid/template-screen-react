{
  "name": "grid-react-app",
  "version": "0.2.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "bootstrap": "mkdirp src/settings && ncp default.settings.json src/settings/index.json",
    "start:dev": "cross-env REACT_APP_DEV_MODE=true react-app-rewired start",
    "start": "yarn schema && cross-env REACT_APP_MESSAGING_URL=ws://localhost:8088 react-app-rewired start",
    "schema": "mkdirp build && npx ts-schema src/schema.ts build && yarn analytics-schema",
    "analytics-schema": "npx tsc src/analytics-schema.ts --moduleResolution node --skipLibCheck --outFile /dev/stdout -m amd | node -e 'a={};eval(\"define=(_,d,c)=>{c(a,a,...d.slice(2).map(require));console.log(JSON.stringify(a.default))};\"+require(\"fs\").readFileSync(\"/dev/stdin\",\"utf8\"))' > build/analytics-schema.json",
    "prodbuild": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build && ncp package.json build/",
    "build": "rimraf build && yarn prodbuild && yarn schema && phy app build",
    "eject": "react-app-rewired eject",
    "pub": "phy app publish",
    "settings": "phy app settings",
    "upload-description": "phy app upload-description",
    "connect": "phy dev ws"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ombori/grid-reports": "^3.37.4",
    "@phygrid/cli": "^4.4.6",
    "@phygrid/hub-client": "^4.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@phygrid/ts-schema": "^4.4.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "ignore-webpack-plugin": "^0.3.0",
    "json-schema-defaults": "^0.4.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.5.2",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "workbox-build": "^7.1.1"
  }
}
